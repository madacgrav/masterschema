{
  "$schema": ".az-env-schema.json",
  "resource_group": {
    "name": "rg-graves",
    "subscriptionName": "ASD Lab"
  },
  "storage_accounts": {
    "moduleVersion": "2022-09-01",
    "storageaccountList": [
        {
          "name": "saadammasterschema",
          "sku": {
              "name": "Standard_LRS"
          },
          "kind": "StorageV2",
          "accessTier": "Hot",
          "allowBlobPublicAccess": false,
          "allowSharedKeyAccess": false,
          "minimumTlsVersion": "TLS1_2",
          "azureFilesIdentityBasedAuthentication": {},
          "networkAcls": { },
          "customDomain": {},
          "dnsEndPointType": "",
          "defaultToOAuthAuthentication": false,
          "encryption": {},
          "extendedLocation": {},
          "immutableStorageWithVersioning": {},
          "isHnsEnabled": false,
          "isLargeFileSharesEnabled": false,
          "routingPreference": {},
          "supportsHttpsTrafficOnly": false,
          "sasPolicy": {},
          "isLocalBackupEnabled": false,
          "isNfsV3Enabled": false,
          "isSfsEnabled": false,
          "keypolicy": {},
          "largeFileSharesState": "Disabled",
          "publicNetworkAccess": "Enabled",
          "routingprefrence": {}
      }
    ]
  },
  "blob_services": [
    {
        "storage_account_parent": "saadammasterschema",
        "cors": {},
        "delete_retention_policy": {},
        "container_delete_retention_policy": {},
        "isversioning_enabled": false
    }
  ],
  "storage_containers": [
    {
        "name": "containeradamtest",
        "storage_account_parent": "saadammasterschema",
        "publicAccess": "None",
        "defaultEncryptionScope": "",
        "denyEncryptionScopeOverride": false,
        "enableNfsV3AllSquash": true,
        "enableNfsV3RootSquash": false        
    }
  ],
  "keyvaults": {
    "moduleVersion": "2021-11-01-preview",
    "keyvaultList":[
      {
        "name": "kvadamtesting",
        "sku_name": "premium",
        "access_policies": [],
        "networkAcls": {}
      }
    ]
  }
}